{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/pi/approve.js": {
      "maxDuration": 10
    },
    "api/pi/complete.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/pi/approve",
      "dest": "/api/pi/approve.js"
    },
    {
      "src": "/api/pi/complete",
      "dest": "/api/pi/complete.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}