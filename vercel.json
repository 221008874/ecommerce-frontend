{
  "functions": {
    "api/pi/approve.js": {
      "runtime": "nodejs18.x"
    },
    "api/pi/complete.js": {
      "runtime": "nodejs18.x"
    }
  },
  "headers": [
    {
      "source": "/api/pi/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/pi/approve",
      "dest": "/api/pi/approve.js"
    },
    {
      "src": "/api/pi/complete",
      "dest": "/api/pi/complete.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}