{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/pi/approve",
      "dest": "/api/approve.js"
    },
    {
      "src": "/api/pi/complete",
      "dest": "/api/complete.js"
    },
    {
      "src": "/api/approve",
      "dest": "/api/approve.js"
    },
    {
      "src": "/api/complete",
      "dest": "/api/complete.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}